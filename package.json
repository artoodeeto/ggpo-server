{
  "name": "ggpo-server",
  "version": "1.0.0",
  "description": "ggpo server",
  "main": "startup.js",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --inspect=0.0.0.0 --respawn --transpileOnly startup.ts",
    "tsc": "tsc",
    "lint": "eslint . --ext .ts",
    "fix:lint": "eslint . --fix --ext .ts",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:show": "ts-node ./node_modules/typeorm/cli.js migration:show",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "test": "jest --runInBand --detectOpenHandles --coverage",
    "typeorm": "TYPEORM_HOST=localhost ts-node ./node_modules/typeorm/cli.js"
  },
  "author": "raphael gako",
  "license": "ISC",
  "dependencies": {
    "@overnightjs/core": "1.6.15",
    "@overnightjs/jwt": "1.1.15",
    "bcrypt": "5.0.0",
    "body-parser": "1.19.0",
    "class-validator": "0.11.0",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-pino-logger": "4.0.0",
    "helmet": "3.21.2",
    "js-yaml": "3.13.1",
    "mysql2": "2.0.1",
    "pino": "5.17.0",
    "reflect-metadata": "0.1.13",
    "swagger-ui-express": "4.1.3",
    "typeorm": "0.2.25",
    "typescript": "3.7.2"
  },
  "devDependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.2",
    "@types/express-jwt": "0.0.42",
    "@types/express-pino-logger": "4.0.2",
    "@types/helmet": "0.0.45",
    "@types/jest": "24.0.23",
    "@types/js-yaml": "3.12.2",
    "@types/jsonwebtoken": "8.3.5",
    "@types/node": "12.12.14",
    "@types/supertest": "2.0.8",
    "@types/swagger-jsdoc": "3.0.2",
    "@types/swagger-ui-express": "4.1.1",
    "@types/yamljs": "0.2.30",
    "@typescript-eslint/eslint-plugin": "2.10.0",
    "@typescript-eslint/parser": "2.10.0",
    "eslint": "6.7.2",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "4.2.1",
    "jest": "24.9.0",
    "jest-extended": "0.11.5",
    "pino-pretty": "3.6.1",
    "prettier": "2.1.2",
    "supertest": "4.0.2",
    "ts-jest": "24.2.0",
    "ts-node-dev": "1.0.0-pre.44"
  }
}

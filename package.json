{
  "name": "ggpo-server",
  "version": "1.0.0",
  "description": "ggpo server",
  "main": "startup.js",
  "private": true,
  "scripts": {
    "dev:docker": "docker-compose up --build",
    "dev": "ts-node-dev --inspect=0.0.0.0 --respawn --transpile-only startup.ts",
    "tsc": "tsc",
    "lint": "eslint . --ext .ts",
    "fix:lint": "eslint . --fix --ext .ts",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:show": "ts-node ./node_modules/typeorm/cli.js migration:show",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "test": "jest --runInBand --detectOpenHandles --coverage",
    "typeorm": "TYPEORM_HOST=localhost ts-node ./node_modules/typeorm/cli.js",
    "prepare": "husky install"
  },
  "author": "raphael gako",
  "license": "ISC",
  "dependencies": {
    "@overnightjs/core": "1.7.5",
    "@overnightjs/jwt": "1.2.0",
    "bcrypt": "5.0.1",
    "class-validator": "0.13.1",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-pino-logger": "6.0.0",
    "helmet": "4.4.1",
    "js-yaml": "4.0.0",
    "mysql2": "2.2.5",
    "passport": "^0.4.1",
    "pino": "6.11.2",
    "reflect-metadata": "0.1.13",
    "swagger-ui-express": "4.1.6",
    "typeorm": "0.2.31",
    "typescript": "4.2.3"
  },
  "devDependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/cors": "2.8.10",
    "@types/express": "4.17.11",
    "@types/express-jwt": "6.0.1",
    "@types/express-pino-logger": "4.0.2",
    "@types/helmet": "^4.0.0",
    "@types/jest": "26.0.22",
    "@types/js-yaml": "4.0.0",
    "@types/jsonwebtoken": "8.5.1",
    "@types/node": "14.14.37",
    "@types/passport": "1.0.7",
    "@types/passport-facebook": "2.1.11",
    "@types/supertest": "2.0.10",
    "@types/swagger-jsdoc": "6.0.0",
    "@types/swagger-ui-express": "4.1.2",
    "@types/yamljs": "0.2.31",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "eslint": "7.23.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "7.0.1",
    "jest": "26.6.3",
    "jest-extended": "0.11.5",
    "pino-pretty": "4.7.1",
    "prettier": "2.2.1",
    "supertest": "6.1.3",
    "ts-jest": "26.5.4",
    "ts-node-dev": "1.1.8"
  }
}

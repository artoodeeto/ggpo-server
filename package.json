    "body-parser": "1.19.0",
    "class-validator": "0.11.0",
    "cookie-parser": "1.4.4",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "helmet": "3.21.2",
    "js-yaml": "3.13.1",
    "mysql2": "2.0.1",
    "reflect-metadata": "0.1.13",
    "swagger-jsdoc": "3.5.0",
    "swagger-ui-express": "4.1.3",
    "typeorm": "0.2.22",
